{
  "name": "@keypad-store/backend",
  "private": true,
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --project tsconfig.json src/index.ts",
    "dev:worker": "ts-node-dev --respawn --transpile-only --project tsconfig.json src/worker.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "worker": "ts-node --project tsconfig.json src/worker.ts",
    "backfill:icon-fields": "ts-node -r dotenv/config scripts/backfill-icon-fields.ts",
    "backfill:insert-asset-id": "ts-node -r dotenv/config scripts/backfill-insert-asset-id.ts",
    "batch:set-catalog-pricing-stock": "ts-node -r dotenv/config scripts/batch-set-catalog-pricing-stock.ts",
    "import:icons": "ts-node -r dotenv/config scripts/import-icons.ts",
    "import:keypads": "ts-node -r dotenv/config scripts/import-keypads.ts",
    "sync:icon-catalog": "ts-node -r dotenv/config scripts/sync-icon-catalog.ts",
    "seed:shop-landing-icons": "ts-node -r dotenv/config scripts/seed-shop-landing-icons.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/lib-storage": "^3.985.0",
    "@nestjs/common": "^11.1.13",
    "@nestjs/graphql": "~13.2.4",
    "@vendure/asset-server-plugin": "3.5.3",
    "@vendure/core": "3.5.3",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "google-auth-library": "^10.5.0",
    "graphql-tag": "^2.12.6",
    "ioredis": "^5.9.2",
    "pg": "^8.18.0",
    "typeorm": "^0.3.21"
  },
  "devDependencies": {
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@biesbjerg/ngx-translate-extract-marker": "^1.0.0",
    "@cds/core": "6.15.1",
    "@clr/angular": "17.3.1",
    "@clr/core": "4.0.15",
    "@clr/icons": "13.0.2",
    "@clr/ui": "17.3.1",
    "@ng-select/ng-select": "14.2.6",
    "@types/pg": "^8.16.0",
    "@vendure/admin-ui": "3.5.3",
    "@vendure/admin-ui-plugin": "3.5.3",
    "@vendure/ui-devkit": "3.5.3",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1",
    "typescript": "5.8.2",
    "xlsx": "^0.18.5",
    "zone.js": "~0.15.0"
  }
}
